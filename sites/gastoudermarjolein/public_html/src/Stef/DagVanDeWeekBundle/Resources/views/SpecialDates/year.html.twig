{% extends "StefDagVanDeWeekBundle::layout.html.twig" %}



{% block content %}
    <div class="row">
        <article class="col-sm-9 maincontent">
            <header class="page-header">
                <h1 class="page-title">Bijzondere dagen {{ year }}</h1>
            </header>

            <h1>Members</h1>

            <table class="table table-striped table-hover">
                {% for date in dates %}
                    {% if date.valid %}
                        <tr>
                            <td>{{ date.description }} {{ year }}</td>
                            <td>{{ date.startDate|date("d/m/Y") }}</td>
                        </tr>
                    {% endif %}
                {% endfor %}
            </table>


        </article>

        <aside class="col-sm-3 sidebar sidebar-right">


        </aside>
    </div>
{% endblock %}


