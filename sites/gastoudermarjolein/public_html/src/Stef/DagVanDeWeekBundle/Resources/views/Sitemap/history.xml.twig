<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
    {% for record in records %}
        <url>
            <loc>http://dagvandeweek.nl{{ path('stef_dag_van_de_week_history_article_show', {
                    'year': record.year,
                    'month': "%02d"|format(record.month),
                    'day': "%02d"|format(record.day),
                    'slug': record.slug
                }) }}
            </loc>
            <changefreq>daily</changefreq>
            <priority>1</priority>
        </url>
    {% endfor %}
</urlset>